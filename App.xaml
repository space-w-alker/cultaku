<Application x:Class="culTAKU.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:culTAKU"
             xmlns:converters="clr-namespace:culTAKU.Misc.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <converters:ImageUriConverter x:Key="ImageLocationConverter"/>
                </ResourceDictionary>
                <ResourceDictionary Source="Styles/dark_skin.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            
        </ResourceDictionary>
    </Application.Resources>
    
    
</Application>
